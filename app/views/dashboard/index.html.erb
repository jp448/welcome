<div class="container mt-5">
  <h1 class="title">Dashboard</h1>
  <h2>Your appointment is on: </h2>
  <% @consultations.each do |consultation| %>
  <%= consultation.date.strftime("%B%d") %>
  <h2>at:</h2>
  <%= consultation.date.strftime("%I:%M%p") %>
  <h2>And your appointment status is:</h2>
  <%= consultation.confirmation %>
  <% end %>

  <li class="icon fa fa-square" ><%= link_to "Messages", chatroom_path(@chatroom), class: "p-2 nav-item"%></li>

</div>
